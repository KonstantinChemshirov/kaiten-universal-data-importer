{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "id": {
        "type": ["string", "number"],
        "description": "Unique identifier for the user."
      },
      "full_name": {
        "type": "string",
        "description": "Full name of the user."
      },
      "email": {
        "type": "string",
        "format": "email",
        "description": "Email address of the user. Must be valid."
      },
      "username": {
        "type": "string",
        "description": "Username."
      }
    },
    "required": ["id", "email"],
    "additionalProperties": false
  }
}
